defun cdf_setup_spectre_simulator_config()
{   
    decl simConfigName = "spectre";
    cdf_add_simulator_config_attribute(simConfigName, "netlistProcedure", "SYMBOL");
    cdf_add_simulator_config_attribute(simConfigName, "otherParameters", "LIST");
    cdf_add_simulator_config_attribute(simConfigName, "instParameters", "LIST");
    cdf_add_simulator_config_attribute(simConfigName, "componentName", "SYMBOL");
    cdf_add_simulator_config_attribute(simConfigName, "termOrder", "LIST");
    cdf_add_simulator_config_attribute(simConfigName, "termMapping", "LIST");
    cdf_add_simulator_config_attribute(simConfigName, "propMapping", "LIST");
    cdf_add_simulator_config_attribute(simConfigName, "stringParameters", "LIST");
    cdf_add_simulator_config_attribute(simConfigName, "opParamExprList", "LIST");
    cdf_add_simulator_config_attribute(simConfigName, "optParamExprList", "LIST");
    cdf_add_simulator_config_attribute(simConfigName, "modelParamExprList", "LIST");
}

