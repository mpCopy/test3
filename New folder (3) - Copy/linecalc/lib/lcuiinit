DATA_DISPLAY_PRECISION = 3
DISPLAY_AS_EXPONENT = 10

LCUI_FREQ_UNITS = Hz kHz MHz GHz
LCUI_LENGTH_UNITS = mil cm mm um meter in ft
LCUI_RES_UNITS = Ohm kOhm
LCUI_ANGLE_UNITS = deg rad

ELEMSUBST = COAXSUB CPWSUB FSUB MSUB RWGSUB SSSUB SSUB SSUBO

BEGIN SUBSTRATE_DESCRIPTION
COAXSUB = Coaxial
CPWSUB = Coplanar Waveguide
FSUB = Finline
MSUB = Microstrip
RWGSUB = Rectangular Waveguide
SSSUB = Suspended Substrate
SSUB = Stripline
END SUBSTRATE_DESCRIPTION

BEGIN MAX_PARM_NUMBER
SUBST = 15
TAND = 1 
MCOVER = 1
MWALL = 2
PERM = 2
SIGMA = 1
END MAX_PARM_NUMBER

ELEMINST = COAX CPW CPWCPL2 CPWCPL4 CPWG BFINL IFINL UFINL MCLIN MLANG MLANG6 MLANG8 MLIN RWG SSCLIN SSLIN SBCLIN SCLIN SLIN SLINO SOCLIN
ELEMINSTSYN = SYNCOUPLER SYNFILTER SYNAMP SYNMIXER SYNRATRACE
ELEMELECCALC = CPWCPL2 CPWCPL4 MCLIN MLANG MLANG6 MLANG8 SBCLIN SCLIN SOCLIN SSCLIN

BEGIN GOAL_MEASUREMENT
BEGIN [BFINL CPW CPWG IFINL MLIN RWG SSLIN UFINL]
Z0
E_Eff
END [BFINL CPW CPWG IFINL MLIN RWG SSLIN UFINL]
BEGIN [CPWCPL2 CPWCPL4 MCLIN MLANG MLANG6 MLANG8 SSCLIN]
Z0
C_DB
E_Eff
END [CPWCPL2 CPWCPL4 MCLIN MLANG MLANG6 MLANG8 SSCLIN]
BEGIN [COAX SLIN SLINO]
Z0
E_Eff
END [COAX SLIN SLINO]
BEGIN [SBCLIN SCLIN SOCLIN]
Z0
C_DB
E_Eff
END [SBCLIN SCLIN SOCLIN]
END GOAL_MEASUREMENT

BEGIN PHYS_FIXED_PARAMETERS
BEGIN [COAX]
PARMS DI
PARMS DO
END [COAX]
BEGIN [CPW]
PARMS W
PARMS G
END [CPW]
BEGIN [CPWG]
PARMS W
PARMS G
END [CPWG]
BEGIN [RWG]
PARMS A
PARMS B
END [RWG]
BEGIN [SLINO]
PARMS W
PARMS S
END [SLINO]
END PHYS_FIXED_PARAMETERS

BEGIN UNITS
FREQ = Freq
LENGTH = Length H Hu Hl T S A B W G L  SkinDepth
RES = Res Z0 ZE ZO
ANGLE = Angle E_Eff
END UNITS

ELEMENT MLIN "MLIN_DEFAULT" 2 UNITS PHYSICAL SUBST MWALL ELECTRICAL RESULT
UNITS UNITS_DEFAULT FREQ_VAL=10.00 FREQ=GHz LENGTH=mil RES=Ohm ANGLE=deg
PHYSICAL W=25.0 mil L=100.0 mil
SUBST MSUB="MSUB_DEFAULT" Er=9.6 Mur=1.0 H=10.0 mil Hu=3.9e7 mil T=0.15 mil Cond=4.1e7 TanD=0 Rough=0 mil DielectricLossModel=1 FreqForEpsrTanD=1.0e9 LowFreqForTanD=1.0e3 HighFreqForTanD=1.0e12
MWALL MWALL="MWALL_DEFAULT" Wall1=n/a mil Wall2=n/a mil
ELECTRICAL Z0=47.25 Ohm E_Eff=230.00 deg
RESULT K_Eff=6.40 A_DB=0.07 SkinDepth=0.0 m

